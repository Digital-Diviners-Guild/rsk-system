        <label>{{localize "RSK.Ranged"}}</label>
        <div>{{system.skills.ranged.level}}</div>
        <input type="checkbox" name="system.skills.ranged.used" value="{{system.skills.ranged.used}}"
          data-dtype="Boolean" {{checked system.skills.ranged.used}}>
        <label>{{localize "RSK.Runecrafting"}}</label>
        <div>{{system.skills.runecrafting.level}}</div>
        <input type="checkbox" name="system.skills.runecrafting.used" value="{{system.skills.runecrafting.used}}"
          data-dtype="Boolean" {{checked system.skills.runecrafting.used}}>
        <label>{{localize "RSK.Slayer"}}</label>
        <div>{{system.skills.slayer.level}}</div>
        <input type="checkbox" name="system.skills.slayer.used" value="{{system.skills.slayer.used}}"
          data-dtype="Boolean" {{checked system.skills.slayer.used}}>
        <label>{{localize "RSK.Smithing"}}</label>
        <div>{{system.skills.smithing.level}}</div>
        <input type="checkbox" name="system.skills.smithing.used" value="{{system.skills.smithing.used}}"
          data-dtype="Boolean" {{checked system.skills.smithing.used}}>
        <label>{{localize "RSK.Summoning"}}</label>
        <div>{{system.skills.summoning.level}}</div>
        <input type="checkbox" name="system.skills.summoning.used" value="{{system.skills.summoning.used}}"
          data-dtype="Boolean" {{checked system.skills.summoning.used}}>
        <label>{{localize "RSK.Thieving"}}</label>
        <div>{{system.skills.thieving.level}}</div>
        <input type="checkbox" name="system.skills.thieving.used" value="{{system.skills.thieving.used}}"
          data-dtype="Boolean" {{checked system.skills.thieving.used}}>
        <label>{{localize "RSK.Woodcutting"}}</label>
        <div>{{system.skills.woodcutting.level}}</div>
        <input type="checkbox" name="system.skills.woodcutting.used" value="{{system.skills.woodcutting.used}}"
          data-dtype="Boolean" {{checked system.skills.woodcutting.used}}>

        <label>{{localize "RSK.Headarmour"}}</label>
        <div>{{system.worn.headarmour}}</div>
        <label>{{localize "RSK.Bodyarmour"}}</label>
        <div>{{system.worn.bodyarmour}}</div>
        <label>{{localize "RSK.Legarmour"}}</label>
        <div>{{system.worn.legarmour}}</div>
        <label>{{localize "RSK.Armarmour"}}</label>
        <div>{{system.worn.armarmour}}</div>
        <label>{{localize "RSK.Weapon"}}</label>
        <div>{{system.worn.weapon}}</div>
        <label>{{localize "RSK.Cape"}}</label>
        <div>{{system.worn.cape}}</div>
        <label>{{localize "RSK.Catalyst"}}</label>
        <div>{{system.catalyst}}</div>
        <label>{{localize "RSK.Motivation"}}</label>
        <div>{{system.motivation}}</div>
        <label>{{localize "RSK.Backgrounds"}}</label>
        <div>{{system.backgrounds}}</div>
        {{#each system.backpack.inventoryslots as |slot key|}}
        <span>.</span>
        {{/each}}
      </div>
</form>