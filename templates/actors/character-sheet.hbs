        <label>{{localize "RSK.Hunter"}}</label>
        <div>{{system.skills.hunter.level}}</div>
        <input type="checkbox" name="system.skills.hunter.used" value="{{system.skills.hunter.used}}"
          data-dtype="Boolean" {{checked system.skills.hunter.used}}>
        <label>{{localize "RSK.Magic"}}</label>
        <div>{{system.skills.magic.level}}</div>
        <input type="checkbox" name="system.skills.magic.used" value="{{system.skills.magic.used}}" data-dtype="Boolean"
          {{checked system.skills.magic.used}}>
        <label>{{localize "RSK.Mining"}}</label>
