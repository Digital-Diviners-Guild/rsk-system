<form class="{{cssClass}} {{actor.type}} flexcol" autocomplete="off">

  {{!-- Sheet Header --}}
  <header class="sheet-header">
    <img class="profile-img" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" height="100" width="100" />
    <div class="header-fields">
      <h1 class="charname"><input name="name" type="text" value="{{actor.name}}" placeholder="Name" /></h1>
      <div>
         <button class="roll-dice"><i class="fas fa-dice"></i> {{localize "RSK.Roll"}}</button>
        <label>{{localize "RSK.Lifepoints"}}</label>
        <div type="number">{{system.stats.lifepoints}}</div>
        <label>{{localize "RSK.Strength"}}</label>
        <div type="number">{{system.stats.strength}}</div>
        <label>{{localize "RSK.Agility"}}</label>
        <div type="number">{{system.stats.agility}}</div>
        <label>{{localize "RSK.Intellect"}}</label>
        <div type="number">{{system.stats.intellect}}</div>
        <label>{{localize "RSK.Summoningpoints"}}</label>
        <div type="number">{{system.stats.summoningpoints}}</div>
        <label>{{localize "RSK.Prayerpoints"}}</label>
        <div type="number">{{system.stats.prayerpoints}}</div>
        <label>{{localize "RSK.Prayerpoints"}}</label>
        <div>{{system.skills.prayerpoints.level}}</div>

        <label>{{localize "RSK.Archaelogy"}}</label>
        <div>{{system.skills.archaelogy.level}}</div>
        <input type="checkbox" name="" value="{{system.skills.archaelogy.used}}" {{checked
          system.skills.archaelogy.used}}>
        <label>{{localize "RSK.Attack"}}</label>
        <div>{{system.skills.attack.level}}</div>
        <input type="checkbox" name="system.skills.attack.used" value="{{system.skills.attack.used}}"
          data-dtype="Boolean" {{checked system.skills.attack.used}}>
        <label>{{localize "RSK.Cooking"}}</label>
        <div>{{system.skills.cooking.level}}</div>
        <input type="checkbox" name="system.skills.cooking.used" value="{{system.skills.cooking.used}}"
          data-dtype="Boolean" {{checked system.skills.cooking.used}}>
        <label>{{localize "RSK.Crafting"}}</label>
        <div>{{system.skills.crafting.level}}</div>
        <input type="checkbox" name="system.skills.crafting.used" value="{{system.skills.crafting.used}}"
          data-dtype="Boolean" {{checked system.skills.crafting.used}}>
        <label>{{localize "RSK.Defence"}}</label>
        <div>{{system.skills.defence.level}}</div>
        <input type="checkbox" name="system.skills.defence.used" value="{{system.skills.defence.used}}"
          data-dtype="Boolean" {{checked system.skills.defence.used}}>
        <label>{{localize "RSK.Dungeoneering"}}</label>
        <div>{{system.skills.dungeoneering.level}}</div>
        <input type="checkbox" name="system.skills.dungeoneering.used" value="{{system.skills.dungeoneering.used}}"
          data-dtype="Boolean" {{checked system.skills.dungeoneering.used}}>
        <label>{{localize "RSK.Farming"}}</label>
        <div>{{system.skills.farming.level}}</div>
        <input type="checkbox" name="system.skills.farming.used" value="{{system.skills.farming.used}}"
          data-dtype="Boolean" {{checked system.skills.farming.used}}>
        <label>{{localize "RSK.Fishing"}}</label>
        <div>{{system.skills.fishing.level}}</div>
        <input type="checkbox" name="system.skills.fishing.used" value="{{system.skills.fishing.used}}"
          data-dtype="Boolean" {{checked system.skills.fishing.used}}>
        <label>{{localize "RSK.Fletching"}}</label>
