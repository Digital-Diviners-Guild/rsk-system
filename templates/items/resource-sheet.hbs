<form class="{{cssClass}}" autocomplete="off">
    <header class="sheet-header">
        <img src="{{item.img}}" data-edit="img" title="{{item.name}}" height="64" width="64" />
        <h1><input type="text" name="name" value="{{item.name}}" placeholder="Name"></h1>
    </header>

    <p>item.system</p>
    <label for="rsk-resource-uses">Uses</label>
    <input id="rsk-resource-uses" type="text" value="{{item.system.uses}}">

    <label for="rsk-resource-effects">Effects</label>
    <input id="rsk-resource-effects" type="text" value="{{item.system.effects}}">

    <label for="rsk-resource-cost">Cost</label>
    <input id="rsk-resource-cost" type="number" value="{{item.system.cost}}">

    <p>data</p>
    <label for="rsk-resource-uses">Uses</label>
    <input id="rsk-resource-uses" type="text" value="{{data.uses}}">

    <label for="rsk-resource-effects">Effects</label>
    <input id="rsk-resource-effects" type="text" value="{{data.effects}}">

    <label for="rsk-resource-cost">Cost</label>
    <input id="rsk-resource-cost" type="number" value="{{data.cost}}">

    {{log item}}
    {{log item.system}}
</form>